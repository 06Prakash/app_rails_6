<h1>Books listing page</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Last Updated Time</th>
      <th>Created Time</th>
    </tr>
  </thead>
  <tbody>
  <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.description %></td>
      <td><%= book.updated_at %></td>
      <td><%= book.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>